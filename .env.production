# AML Engine Production Environment Variables

# Security
SECRET_KEY=
AUTH_ENABLED=

# Database
DB_USER=
DB_PASSWORD=
DATABASE_URL=postgresql://aml_user:@postgres:5432/aml_engine

# Redis
REDIS_URL=redis://redis:6379

# Monitoring
GRAFANA_PASSWORD=

# SSL Certificates (update with your domain)
SSL_CERT_PATH=/etc/nginx/ssl/cert.pem
SSL_KEY_PATH=/etc/nginx/ssl/key.pem

# Logging
LOG_LEVEL=INFO
LOG_RETENTION_DAYS=30

# Performance
MAX_WORKERS=4
WORKER_TIMEOUT=300
